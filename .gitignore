# Ignore bundler dependencies
/vendor/bundle
/node_modules

# Ignore logs and temp files
/log/*
!/log/.keep
/tmp
!/tmp/.keep
/tmp/pids/*
/tmp/cache/*
/tmp/sockets/*

# Ignore Byebug history
.byebug_history

# Ignore storage (ActiveStorage, if used)
/storage/*
!/storage/.keep

# Ignore system files
.DS_Store
Thumbs.db

# Ignore editor and IDE files
*.swp
*.swo
*~
.vscode/
.idea/
*.iml

# Ignore credentials and secrets
/config/master.key
/config/credentials.yml.enc

# Ignore database files
*.sqlite3
*.sqlite3-journal

# Ignore testing and coverage reports
/coverage/
/spec/examples.txt

# Ignore AWS & Terraform (if applicable)
/.aws/
terraform.tfstate*

# Ignore compiled assets
/public/assets
/public/packs
/public/packs-test
/node_modules/
yarn-error.log
npm-debug.log